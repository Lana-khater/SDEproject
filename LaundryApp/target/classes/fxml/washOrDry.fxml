<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainANDcontrollers.WashOrDry">
   <children>
      <Button fx:id="washingButton" layoutX="-2.0" layoutY="-2.0" mnemonicParsing="false" onAction="#washingButtonAction" opacity="0.2" prefHeight="606.0" prefWidth="357.0" style="-fx-background-color: #0492C2;" text="Washing" textFill="#ea0303">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font>
      </Button>
      <Button fx:id="dryingButton" alignment="CENTER" layoutX="348.0" layoutY="-2.0" mnemonicParsing="false" onAction="#dryingButtonAction" prefHeight="606.0" prefWidth="357.0" style="-fx-background-color: #B87333;" text="Drying" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font>
      </Button>
      <Label fx:id="label" alignment="CENTER" layoutX="200.0" layoutY="44.0" prefHeight="53.0" prefWidth="300.0" text="Pick and go !!" textAlignment="CENTER" textFill="#0d0d0d">
         <font>
            <Font name="Candara Light" size="34.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
